	RULE 67 : IF INKYdanger IS low AND INKYdistanceToPacman IS far THEN Flank IS emergency;
	RULE 68 : IF SUEdanger IS low AND SUEdistanceToPacman IS far THEN Flank IS emergency;
	RULE 69 : IF BLINKYdanger IS low AND BLINKYdistanceToPacman IS medium THEN Flank IS nervious;
	RULE 70 : IF PINKYdanger IS low AND PINKYdistanceToPacman IS medium THEN Flank IS nervious;
	RULE 71 : IF INKYdanger IS low AND INKYdistanceToPacman IS medium THEN Flank IS nervious;
	RULE 72 : IF SUEdanger IS low AND SUEdistanceToPacman IS medium THEN Flank IS nervious;
	RULE 73 : IF BLINKYdanger IS low THEN Flee IS calm;
	RULE 74 : IF PINKYdanger IS low THEN Flee IS calm;
	RULE 75 : IF INKYdanger IS low THEN Flee IS calm;
	RULE 76 : IF SUEdanger IS low THEN Flee IS calm;
	RULE 77 : IF PacmanDistanceToPPill IS near AND BLINKYdistanceToPacman IS medium THEN Disperse IS nervious;
